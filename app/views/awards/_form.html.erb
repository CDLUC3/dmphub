<%= f.fields_for :awards, Award.new do |a| %>
  <%= a.label :funder_name, 'Funding Agency' %>
  <%= a.text_field :funder_name, href: fundref_autocomplete_path %>
  <%= a.hidden_field :funder_uri, class: 'autocomplete-funder_name' %>
  <%= a.label :status %>
  <%= a.select :status, Award.statuses.keys %>
  <%= a.fields_for :identifiers, Identifier.new do |i| %>
    <%= i.hidden_field :category, value: 'url' %>
    <%= i.label :value, 'URI' %>
    <%= i.text_field :value %>
  <% end %>
<% end %>
